<extension xmlns="http://ns.adobe.com/air/extension/21.0">
    <id>jp.appAdForce</id>
    <versionNumber>1.0.0</versionNumber>
    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment> 
                <nativeLibrary>AppAdForce.jar</nativeLibrary> 
                <initializer>jp.appAdForce.android.ane.AppAdForceExtension</initializer> 
                <finalizer>jp.appAdForce.android.ane.AppAdForceExtension</finalizer>
            </applicationDeployment> 
        </platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libFoxANE.a</nativeLibrary>
				<initializer>foxInitilize</initializer>
				<finalizer>foxFinalize</finalizer>
			</applicationDeployment>
		</platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
